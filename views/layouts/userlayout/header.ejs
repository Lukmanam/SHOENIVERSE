            <li class="nav-item cta cta-colored">
              <a href="/cart" class="nav-link"
                ><span
                  class="icon-shopping_cart"
                  style="font-size: 18.5px"
                ></span
              ></a>
            </li>
            <li class="nav-item cta cta-colored">
              <a href="/wishlist" class="nav-link"
                ><span class="icon-heart" style="font-size: 18.5px"></span
              ></a>
